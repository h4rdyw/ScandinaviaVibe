<!DOCTYPE html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="{{ page_description | escape }}" />
    <link rel="canonical" href="{{ canonical_url }}" />
    <meta
      name="viewport"
      content="width=device-width,initial-scale=1,shrink-to-fit=no"
    />
    {% comment %}theme-check-disable{% endcomment %}
    {% section 'announcement-bar' %}

    {{ content_for_header }}
    <!-- Header hook for plugins -->
    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
    {{ 'application.css.scss' | asset_url | stylesheet_tag }}
    {{ 'font-style.css' | asset_url | stylesheet_tag }}

    {{ 'iframeResizer.min.js' | asset_url | script_tag }}
    {{ 'iframeResizer.contentWindow.js' | asset_url | script_tag }} 
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    
  </head>
  <body>
    {% section 'translation' %}
    {% section 'header' %}

    <main role="main">
      {{ content_for_layout }}
    </main>
    
    {% section 'footer-section' %}

    {{ 'bootstrap.min.js' | asset_url | script_tag }}
    {{ 'application.js' | asset_url | script_tag }}
    
    {% comment %} <script type="module" src="{{ 'currency.js' | asset_url }}">
      import {formatMoney} from 'currency';
      const ccy = formatMoney(1133, theme.moneyFormat);
      console.log(ccy); 
    </script> {% endcomment %}
{% comment %}theme-check-enable{% endcomment %}
  </body>
</html>
