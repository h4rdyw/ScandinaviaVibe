{% comment %}theme-check-disable{% endcomment %}
<!DOCTYPE html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="{{ page_description | escape }}" />
    <link rel="canonical" href="{{ canonical_url }}" />
    <!-- <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.css"/> -->
    <!-- <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css"/> -->
    <!-- <link rel="stylesheet" href="node_modules/lite-youtube-embed/src/lite-yt-embed.css" /> -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <!-- <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script> -->
    <!-- <script src="node_modules/lite-youtube-embed/src/lite-yt-embed.js"></script> -->
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
    <!-- <script src="https://unpkg.com/vue@3.0.5"></script> -->

    <meta
      name="viewport"
      content="width=device-width,initial-scale=1,shrink-to-fit=no"
    />

    {{ content_for_header }}
    {% section 'header' %}
    {% section 'announcement-bar' %}

    <!-- Header hook for plugins -->
    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
    {{ 'application.css.scss' | asset_url | stylesheet_tag }}
    {{ 'font-style.css' | asset_url | stylesheet_tag }}   
    {{ 'lite-yt-embed.css' | asset_url | stylesheet_tag }}     
  </head>
  <body>
   
    <main role="main">
      {{ content_for_layout }}
    </main>
    
    <div id="btn-back-to-top">{{ 'icon_up.png' | asset_img_url: '32px' | img_tag }}</div>
    {% section 'footer-section' %}
    {% section 'translation' %}

    {{ 'bootstrap.min.js' | asset_url | script_tag }}
    {{ 'application.js' | asset_url | script_tag }}
    {{ 'lite-yt-embed.js' | asset_url | script_tag }}
    
  </body>
</html>
{% comment %}theme-check-enable{% endcomment %}
